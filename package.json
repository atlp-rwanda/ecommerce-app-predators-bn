{
  "name": "ecommerce-app-predators-bn",
  "version": "1.0.0",
  "description": "predator ecommerce project",
  "main": "server.js",
  "type": "module",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "make-model": "npx sequelize-cli model:generate --name user --attributes email:string",
    "migrate": "npx sequelize-cli db:migrate",
    "make-seed": "npx sequelize-cli seed:generate --name",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo",
    "down": "npx sequelize-cli db:migrate:undo",
    "test": "cross-env NODE_ENV=test jest",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --experimental-modules --es-module-specifier-resolution=node src/server.js",
    "build": "babel src -d dist",
    "deploy": "cross-env NODE_ENV=production babel-node src/server.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/ecommerce-app-predators-bn.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/ecommerce-app-predators-bn/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/ecommerce-app-predators-bn#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "eslint-config-prettier": "^8.8.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "i18next": "^22.4.13",
    "i18next-express-middleware": "^2.0.0",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.3.0",
    "i18next-node-fs-backend": "^2.1.3",
    "jest": "^29.5.0",
    "jsend": "^1.1.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "openapi-types": "^7.0.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.30.0",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@types/swagger-ui-express": "^4.1.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7"
  }
}
